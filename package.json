{
  "name": "gitlit",
  "version": "2.1.3",
  "description": "Simple GUI for Git LFS",
  "main": "app/main.js",
  "build": {
    "appId": "pirol541.gitlit",
    "linux": {
      "target": [
        {
          "target": "tar.gz",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "bootstrap": "^4.6.0",
    "ejs": "^3.1.5",
    "ejs-render-remote": "^1.0.13",
    "electron-find": "^1.0.6",
    "electron-localshortcut": "^3.2.1",
    "electron-store": "^7.0.2",
    "github-app-updater": "^1.0.5",
    "jquery": "^3.5.1",
    "material-design-icons": "^3.0.1",
    "minimist": "^1.2.5",
    "nonblockjs": "^1.0.8",
    "pnotify": "^4.0.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "electron": "^11.2.1",
    "electron-builder": "^22.11.7"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --win nsis --dir --publish=never",
    "dist": "electron-builder --win nsis --publish=never"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pirol541/gitlit"
  },
  "author": "pirol541",
  "license": "ISC"
}
